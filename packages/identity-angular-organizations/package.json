{
  "name": "@identity-base/angular-organizations",
  "version": "0.4.0",
  "description": "Angular organizations client for Identity.Base (built on @identity-base/angular-client).",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external @angular/core --external @angular/common/http --external rxjs --external rxjs/operators --external @identity-base/client-core --external @identity-base/angular-client",
    "test": "npm run build && node --test test/*.test.mjs",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run typecheck"
  },
  "keywords": [
    "angular",
    "organizations",
    "multitenant",
    "identity"
  ],
  "author": "Identity Base",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0",
    "@angular/common": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0",
    "rxjs": "^7.0.0",
    "@identity-base/client-core": "0.4.0",
    "@identity-base/angular-client": "0.4.0"
  },
  "devDependencies": {
    "@identity-base/client-core": "file:../identity-client-core",
    "@identity-base/angular-client": "file:../identity-angular-client",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Amaretto-Software-Labs/identity-base"
  }
}
